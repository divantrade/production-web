{
  "name": "production-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "production-check": "node scripts/production-check.js",
    "pre-deploy": "npm run type-check && npm run lint && npm run test:all && npm run production-check",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@sanity/client": "^7.11.1",
    "@sanity/color-input": "^4.0.6",
    "@sanity/image-url": "^1.2.0",
    "@sanity/ui": "^3.1.0",
    "@sanity/vision": "^4.9.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.13",
    "next": "15.5.12",
    "next-intl": "^4.8.2",
    "next-sanity": "^11.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "sanity": "^4.9.0",
    "sanity-plugin-icon-picker": "^4.0.0",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@sentry/nextjs": "^10.12.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.12",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-testing-library": "^7.8.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.6",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
